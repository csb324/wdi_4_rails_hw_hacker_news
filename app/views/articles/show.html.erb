<%= render 'article_box', article: @article %>
<p>Created by user #<%= @article.user_id %> </p>

<% if user_signed_in? %>
  <% if @article.user == current_user %>
    <%= link_to "edit", edit_article_path %>
  <% end %>
<% end %>
<% @article.comments.each do |comment| %>
  <p><%= comment.user.email %> posted:</p>
  <p><%= comment.content %></p>
  <br>
<% end %>
